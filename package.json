{
  "name": "pw-pomish-starter",
  "version": "0.1.0",
  "private": true,
  "type": "commonjs",
  "scripts": {
    "install:playwright": "npx playwright install --with-deps",
    "test": "playwright test",
    "test:ui": "playwright test --ui",
    "test:headed": "playwright test --headed",
    "test:e2e": "playwright test tests/e2e",
    "test:api": "playwright test tests/api",
    "test:api:ui": "cross-env LOG_API_UI=true playwright test tests/api --ui",
    "test:api:report": "cross-env LOG_API_REPORT=true playwright test tests/api",
    "test:api:dark": "cross-env COLOR_SCHEME=dark playwright test tests/api",
    "test:api:accessible": "cross-env COLOR_SCHEME=accessible playwright test tests/api",
    "test:visual": "playwright test tests/visual",
    "test:visual:applitools": "cross-env APPLITOOLS_API_KEY=m98taTZLgopPteCGa3v1RKHi1hq8YiQtWoBLaobx1GJk110 playwright test tests/visual/applitools",
    "test:visual:all": "playwright test tests/visual && cross-env APPLITOOLS_API_KEY=m98taTZLgopPteCGa3v1RKHi1hq8YiQtWoBLaobx1GJk110 playwright test tests/visual/applitools",
    "show-report": "playwright show-report",
    "show-ortoni-report": "start ortoni-report/index.html",
    "format": "prettier --write .",
    "lint": "eslint . --ext .ts",
    "typecheck": "tsc --noEmit",
    "codegen": "playwright codegen https://automationintesting.online/",
    "test:chromium": "playwright test --project=rbp-chromium",
    "test:edge": "playwright test --project=rbp-edge",
    "test:webkit": "playwright test --project=rbp-webkit",
    "test:mobile": "playwright test --project=rbp-mobile-chrome --project=rbp-mobile-safari",
    "test:applitools:chromium": "cross-env APPLITOOLS_API_KEY=m98taTZLgopPteCGa3v1RKHi1hq8YiQtWoBLaobx1GJk110 playwright test --project=rbp-applitools-chromium",
    "test:applitools:edge": "cross-env APPLITOOLS_API_KEY=m98taTZLgopPteCGa3v1RKHi1hq8YiQtWoBLaobx1GJk110 playwright test --project=rbp-applitools-edge",
    "test:applitools:webkit": "cross-env APPLITOOLS_API_KEY=m98taTZLgopPteCGa3v1RKHi1hq8YiQtWoBLaobx1GJk110 playwright test --project=rbp-applitools-webkit",
    "test:all": "playwright test",
    "test:allure": "cross-env REPORTER_TYPE=allure playwright test && npm run allure:generate && npm run enhance-allure",
    "test:ortoni": "cross-env REPORTER_TYPE=ortoni playwright test && npm run enhance-ortoni",
    "enhance-ortoni": "ts-node scripts/enhance-ortoni.ts",
    "enhance-allure": "ts-node scripts/enhance-allure.ts",
    "test:steps": "cross-env REPORTER_TYPE=steps playwright test",
    "test:monocart": "cross-env REPORTER_TYPE=monocart playwright test",
    "allure:generate": "allure generate allure-results --clean -o allure-report",
    "allure:open": "allure open allure-report",
    "allure:serve": "allure serve allure-results",
    "allure:clean": "rimraf allure-results allure-report",
    "steps:open": "start steps-report/summary.html",
    "steps:clean": "rimraf steps-report",
    "monocart:open": "start monocart-report/index.html",
    "monocart:clean": "rimraf monocart-report",
    "test:unit": "jest",
    "test:unit:watch": "jest --watch",
    "test:unit:coverage": "jest --coverage"
  },
  "devDependencies": {
    "@applitools/eyes-playwright": "^1.40.5",
    "@eslint/js": "^9.11.0",
    "@playwright/test": "^1.48.0",
    "@types/ejs": "^3.1.5",
    "@types/jest": "^30.0.0",
    "@types/node": "^22.5.0",
    "@typescript-eslint/eslint-plugin": "^8.0.0",
    "@typescript-eslint/parser": "^8.0.0",
    "allure-commandline": "^2.34.1",
    "allure-playwright": "^3.4.1",
    "cross-env": "^10.1.0",
    "dotenv": "^17.2.3",
    "ejs": "^3.1.10",
    "eslint": "^9.11.0",
    "eslint-config-prettier": "^9.1.0",
    "eslint-plugin-import": "^2.29.1",
    "eslint-plugin-unused-imports": "^4.0.0",
    "jest": "^30.2.0",
    "monocart-reporter": "^2.9.23",
    "ortoni-report": "^4.0.1",
    "playwright-schema-validator": "^1.0.0",
    "prettier": "^3.3.3",
    "pw-api-plugin": "^2.1.0",
    "rimraf": "^6.0.1",
    "ts-jest": "^29.4.5",
    "ts-node": "^10.9.2",
    "typescript": "^5.6.3"
  }
}
